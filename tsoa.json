{
  "entryFile": "src/server.ts",
  "spec": {
    "outputDirectory": "./src/docs",
    "specVersion": 3,
    "securityDefinitions": {
      "api_key": {
        "type": "apiKey",
        "name": "X-API-Key",
        "in": "header"
      }
    }
  },
  "routes": {
    "basePath": "/api",
    "entryFile": "./src/server.ts",
    "routesDir": "./src",
    "authenticationModule": "./src/authentication.ts"
  }
}
